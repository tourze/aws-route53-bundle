{
  "name": "tourze/aws-route53-bundle",
  "description": "Multi-account AWS Route53 DNS management bundle with bidirectional synchronization",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "async-aws/route53": "^2.0",
    "doctrine/collections": "^2.3",
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^3.0",
    "easycorp/easyadmin-bundle": "^4",
    "knplabs/knp-menu": "^3.7",
    "psr/log": "^3|^2|^1",
    "symfony/cache": "^7.3",
    "symfony/config": "^7.3",
    "symfony/console": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/doctrine-bridge": "^7.3",
    "symfony/event-dispatcher": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-client-contracts": "^3.6",
    "symfony/http-foundation": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/lock": "^7.3",
    "symfony/messenger": "^7.3",
    "symfony/serializer": "^7.3",
    "symfony/uid": "^7.3",
    "symfony/validator": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/bundle-dependency": "1.*",
    "tourze/doctrine-indexed-bundle": "1.0.*",
    "tourze/easy-admin-menu-bundle": "0.1.*"
  },
  "require-dev": {
    "doctrine/data-fixtures": "^2.0",
    "doctrine/doctrine-fixtures-bundle": "^4.0",
    "doctrine/persistence": "^4.1",
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "symfony/http-client": "^7.3",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-doctrine-entity": "1.*",
    "tourze/phpunit-symfony-kernel-test": "1.*",
    "tourze/phpunit-symfony-unit-test": "1.*",
    "tourze/phpunit-symfony-web-test": "1.*",
    "tourze/symfony-dependency-service-loader": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\AwsRoute53Bundle\\": "src"
